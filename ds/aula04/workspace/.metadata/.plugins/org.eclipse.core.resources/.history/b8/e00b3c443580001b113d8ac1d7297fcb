package basico;

public class Animal {

	String especie;
	String raca;
	String nome;
	float peso;
	
	//Construtor simples
	public Animal() {
		especie = "Gato";
		nome = "Mimi";
		raca = "Angorá";
		peso = (float) 1.2;
	}
	
	//Construtor com parâmetros
	public Animal(String especie, String nome, String raca, float peso) {
		this.especie = especie;
		this.nome = nome;
		this.raca = raca;
		this.peso = peso;
	}
	
}
